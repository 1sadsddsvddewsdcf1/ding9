			{
				if(children[i].id=='nowrap')
				{
					if(document.all){
						children[i].attachEvent ('onmouseover', doMove);
					}
					else{
						children[i].addEventListener('mouseover', doMove, false);
					}
				}
			}
		}
		add_Move();
		//-->
		</SCRIPT>
 
 
