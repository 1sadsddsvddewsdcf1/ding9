	document.searchmerchant.action="http://search.ding9.com/merchant/merchant-info.do";
	return true;
}

function other(obj,strtype)
{
	if(trim(obj.key.value)=='' || obj.key.value=='输入关键字进行搜索'){
		alert('请输入查询关键字');
		return false;
	}	
	url="http://search.ding9.com/" + strtype + ".do?q=" + obj.key.value;
	if(strtype=='comment') url+='&cid='+obj.prso_id_one.value;
	obj.action=url;
	return true;
}
